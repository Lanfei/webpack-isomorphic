{
  "name": "webpack-isomorphic-react-example",
  "version": "4.2.0",
  "description": "The example of webpack-isomorphic.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf views/dist",
    "babel": "babel views/src -d views/dist",
    "babel:watch": "babel views/src -d views/dist --watch",
    "webpack": "webpack --progress --config views/webpack.prod.js",
    "webpack:watch": "webpack --watch --config views/webpack.prod.js",
    "webpack:dev": "webpack --progress --config views/webpack.dev.js",
    "webpack:dev:watch": "webpack --watch --config views/webpack.dev.js",
    "webpack:dev:server": "webpack-dev-server --config views/webpack.dev.js",
    "build": "npm-run-all clean babel webpack",
    "build:dev": "npm-run-all clean babel webpack:dev",
    "watch": "npm-run-all clean --parallel babel:watch webpack:watch",
    "watch:dev": "npm-run-all clean --parallel babel:watch webpack:dev:watch",
    "start": "node index.js",
    "start:dev": "npm-run-all clean --parallel babel:watch webpack:dev:server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jealous",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}

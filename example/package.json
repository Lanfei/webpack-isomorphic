{
  "name": "webpack-isomorphic-example",
  "version": "4.0.1",
  "description": "The example of webpack-isomorphic.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf views/dist",
    "babel": "babel views/src -d views/dist",
    "babel:watch": "babel views/src -d views/dist --watch",
    "webpack": "webpack --progress --config webpack.prod.js",
    "webpack:watch": "webpack --watch --config webpack.prod.js",
    "webpack:dev": "webpack --progress --config webpack.dev.js",
    "webpack:dev:watch": "webpack --watch --config webpack.dev.js",
    "webpack:dev:server": "webpack-dev-server --config webpack.dev.js",
    "build": "npm-run-all clean babel webpack",
    "build:dev": "npm-run-all clean babel webpack:dev",
    "watch": "npm-run-all clean --parallel babel:watch webpack:watch",
    "watch:dev": "npm-run-all clean --parallel babel:watch webpack:dev:watch",
    "start": "node index.js",
    "start:dev": "npm-run-all clean --parallel babel:watch webpack:dev:server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jealous",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.5",
    "npm-run-all": "^4.1.5",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  }
}

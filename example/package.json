{
  "name": "webpack-isomorphic-example",
  "version": "3.0.0",
  "description": "The example of webpack-isomorphic.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf views/dist",
    "dev": "npm run clean && babel views/src -d views/dist -w & NODE_ENV=development webpack-dev-server --config webpack.dev.js",
    "build": "npm run clean && babel views/src -d views/dist && webpack --progress --config webpack.prod.js",
    "watch": "npm run clean && babel views/src -d views/dist --watch & webpack --watch --config webpack.prod.js",
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jealous",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0",
    "webpack-merge": "^4.1.1"
  }
}
